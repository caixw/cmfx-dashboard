{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "outDir": "lib",
        "types": ["vite/client"],
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "declaration": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
                "@dashboard/*": ["./*"]
            }
        },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
